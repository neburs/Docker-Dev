[color]
    ui = auto
[color "branch"]
    current = yellow bold
    local = yellow
    remote = green
[color "diff"]
    meta = yellow
    frag = yellow bold
    old = red bold
    new = blue bold
[color "status"]
    added = green
    changed = red
    untracked = cyan
[alias]
    st = status
    ci = commit
    br = branch
    co = checkout
    df = diff
    ds = diff --staged
    lg = log -p
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    lola = log --graph --decorate --pretty=oneline --abbrev-commit --all --date=local
    lolaa = log --graph --decorate --pretty=fuller --abbrev-commit --all --date=local
    ls = ls-files
    unstage = reset HEAD
